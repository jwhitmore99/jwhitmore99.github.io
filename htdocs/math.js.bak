function multiplyBy()
{
	num1 = document.getElementById("firstNumber").value;
	num2 = document.getElementById("secondNumber").value;
	document.getElementById("result").innerHTML = num1 * num2;
}

function divideBy()
{
	num1 = document.getElementById("firstNumber").value;
	num2 = document.getElementById("secondNumber").value;
	document.getElementById("result").innerHTML = num1 / num2;
}

function primeBy()
{
	public class Main {

	  public static void main(String[] args) {
		num3 = document.getElementById("primeNumber").value;
		int num = num3, i = 2;
		boolean flag = false;
		while (i <= num / 2) {
		  // condition for nonprime number
		  if (num % i == 0) {
			flag = true;
			break;
		  }

		  ++i;
		}

		if (!flag)
		  document.getElementById("result").innerHTML = System.out.println(num + " is a prime number.");
		else
		  document.getElementById("result").innerHTML = System.out.println(num + " is not a prime number.");
	  }
}
}

function squareBy()
{
	num1 = document.getElementById("firstNumber").value;
	document.getElementById("result").innerHTML = num1 * num1;
}
